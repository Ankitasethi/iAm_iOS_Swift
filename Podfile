pod 'FirebaseUI'
pod 'FBSDKCoreKit', '~> 4.39.1'

  target 'finalcryTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'finalcryUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
